<owo-ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/wisp-forest/owo-lib/1.20/owo-ui.xsd">
    <components>
        <flow-layout direction="vertical">
            <children>
                <label  id="version">
                    <text>RealmsAddon v0.4</text>
                    <margins>
                        <all>8</all>
                    </margins>
                </label>
                <flow-layout direction="horizontal">
                    <children>
                        <button id="Home">
                            <text>Home</text>
                            <margins>
                                <left>5</left>
                                <top>5</top>
                            </margins>
                            <sizing>
                                <horizontal method="fixed">100</horizontal>
                            </sizing>
                        </button>
                        <button id="Gui">
                            <text>Gui</text>
                            <margins>
                                <left>5</left>
                                <top>5</top>
                            </margins>
                            <sizing>
                                <horizontal method="fixed">100</horizontal>
                            </sizing>
                        </button>
                        <button id="Settings">
                            <text>Settings</text>
                            <margins>
                                <left>5</left>
                                <top>5</top>
                            </margins>
                            <sizing>
                                <horizontal method="fixed">100</horizontal>
                            </sizing>
                        </button>
                        <button id="Range">
                            <text>Range</text>
                            <margins>
                                <left>5</left>
                                <top>5</top>
                            </margins>
                            <sizing>
                                <horizontal method="fixed">100</horizontal>
                            </sizing>
                        </button>
                    </children>
                </flow-layout> <!--Tab buttons-->

                <flow-layout direction="vertical" id="SettingsContentPane">
                    <children>

                            <checkbox id="SwingSetting">
                                <text>Auto Swing</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>

                            <checkbox id="SwingIfNoCooldown">
                                <text>Swing Only If No Cooldown</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>

                            <checkbox id="GammaSetting">
                                <text>Gamma</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="FPSSetting">
                                <text>Show FPS</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="PingSetting">
                                <text>Show Ping</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="PlaytimeSetting">
                                <text>Show Playtime</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="SpawnBossesSetting">
                                <text>Show Spawned Bosses</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="BossWaypointsSetting">
                                <text>Show Boss Waypoints</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="SoundSetting">
                                <text>Custom Bag Sounds</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="DiscordRPCSetting">
                                <text>Discord Rich Presence</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <flow-layout direction="horizontal">
                                <children>
                                    <label id="DiscordDefaultStatusLabel">
                                        <text>Set Discord Status: </text>
                                    </label>
                                    <text-box id="DiscordDefaultStatusMessage">
                                        <text>a</text>
                                        <sizing>
                                            <horizontal method="fixed">150</horizontal>
                                        </sizing>
                                    </text-box></children>
                            </flow-layout>
                            <checkbox id="RPCShowLocationSetting">
                                <text>Show location</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="RPCShowFightingSetting">
                                <text>Show current boss</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="ToggleHotkeyShoutSetting">
                                <text>Call hotkey shout</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>
                            <checkbox id="DungeonTimerSetting">
                                <text>Dungeon Timer</text>
                                <sizing>
                                    <horizontal method="fixed">150</horizontal>
                                </sizing>
                            </checkbox>

                    </children>

                    <padding><all>10</all></padding>
                </flow-layout>
            </children>
            <surface>
                <vanilla-translucent/>
            </surface>
        </flow-layout>
    </components>
</owo-ui>
